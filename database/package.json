{
  "name": "network-scanner-db",
  "version": "1.0.0",
  "description": "Database initialization and seeding scripts",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "migrate": "node scripts/migrate.js",
    "db:init": "node scripts/migrate.js",
    
    "db:seed-all": "node seed/complete_seed.js",
    "db:seed-so": "node seed/complete_seed.js so",
    "db:seed-oui": "node seed/complete_seed.js oui",
    "db:seed-protocols": "node seed/complete_seed.js protocols",
    
    "db:full-init": "npm run db:init && npm run db:seed-all",
    
    "db:status": "node seed/complete_seed.js status",
    "db:verify": "node scripts/verify_seed.js"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.0",
    "mysql2": "^3.6.0",
    "csv-parser": "^3.0.0"
  }
}
